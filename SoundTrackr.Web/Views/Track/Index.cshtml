@model IEnumerable<SoundTrackr.Service.DTOs.TrackDTO>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TrackStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrackEnd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartState)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TrackStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrackEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartCity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartState)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
